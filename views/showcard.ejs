<script>

    const btn = document.querySelector('.lihasmove-btn')
    const move = document.querySelector('.lihasmove')

    let moved = true
    btn.addEventListener('click', function(){
        if(moved){
            move.style.transform = 'translateX(-250px)'
            moved = !moved
        }
        else {
            move.style.transform = 'translateX(0)'
            moved = !moved
        }

        
    })





</script>