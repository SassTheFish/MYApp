<% layout('layout/boilerplateUL.ejs')%> 


<div id="showcard">
    <div id="sc-grid">
        <% if (type === 'ujumine') { %>
            <header>
                <div id="sc-level"><%=kava.pingutustase%>%</div>
                <h4 id="sc-title"><%= kava.nimi %> </h4>
            </header>
            <main id="sc-harj" class="sc-ujumine">
                <ul>
                    <li class="thead">
                        <span class="kordus">Kordused</span>
                        <span class="ala">Ala</span>
                        <span class="harjutus">Eraldus</span>
                        <span class="eraldus">Harjutus</span>
                        <span class="vahendid">Vahendid</span>
                        <span class="ajastus">Ajastus</span>
                    </li>
                    <% for(harj of kava.harjutused) { %>
                        <li>
                            <span class="kordus"><%= harj.kordused %> </span>
                            <span class="ala"><%= harj.ala %> </span>
                            <span class="harjutus"><%= harj.eraldus %> </span>
                            <span class="eraldus"><%= harj.harjutus %> </span>
                            <span class="vahendid"><%= harj.vahendid %> </span>
                            <span class="ajastus"><%= harj.ajastus %> </span>
                        </li>
                        <hr>
                    <% } %>
                </ul>
            </main>
            <footer>
                <div id="sc-buttons">
                    <a id="button-tagasi" href="/ujumisekavad" style="grid-column: 4;">Tagasi</a>
                </div>
            </footer>
        <% } %>
